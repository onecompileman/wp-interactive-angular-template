{
    "name": "pcp-convention-angular",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --hmr=true --host 0.0.0.0",
        "build-windows": "ng build --prod && move /y ../../public/frontend/index.html ../views/frontend.blade.php && rmdir /s /q \"../../public/assets\"  && move /y ../../public/frontend/* ../../public && move /y ../../public/frontend/assets ../../public",
        "build-mac": "ng build --prod && mv ../../public/frontend/index.html ../views/frontend.blade.php && sudo rm -r ../../public/assets && mv  ../../public/frontend/* ../../public",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
